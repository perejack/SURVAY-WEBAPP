{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "web-build",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|ico|json)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
